{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "username": {
        "type": "string",
        "maxLength": 100
      },
      "password": {
        "type": "string",
        "maxLength": 100
      }
    },
    "required": ["username", "password"]
}
  